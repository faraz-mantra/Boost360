<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  package="com.dashboard">

  <uses-permission android:name="android.permission.INTERNET" />

<!--  <permission-->
<!--    android:name="com.dashboard.CUSTOM_PERMISSION"-->
<!--    android:label="dashboard_permission"-->
<!--    android:protectionLevel="dangerous" />-->

  <application
    android:requestLegacyExternalStorage="true"
    tools:ignore="UnusedAttribute">

    <activity
      android:name=".controller.DashboardActivity"
      android:exported="true"
      android:label="@string/dashboard"
      android:launchMode="singleTask"
      android:screenOrientation="portrait"
      android:theme="@style/DashboardTheme"
      android:windowSoftInputMode="adjustPan">
      <intent-filter
        android:autoVerify="true"
        android:label="@string/app_name">
        <action android:name="android.intent.action.VIEW" />

        <category android:name="android.intent.category.DEFAULT" />
        <category android:name="android.intent.category.BROWSABLE" />

        <data
          android:host="${applicationId}.keyboard.home"
          android:pathPrefix="/"
          android:scheme="http" />

        <data
          android:host="boost.nowfloats.com"
          android:pathPrefix="/"
          android:scheme="http" />

        <data
          android:host="nowfloat.onelink.me"
          android:pathPrefix="/FX0L"
          android:scheme="https" />
      </intent-filter>
      <intent-filter>
        <action android:name="com.dashboard.controller.DashboardActivity" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>

    <activity
      android:name=".controller.DashboardFragmentContainerActivity"
      android:screenOrientation="portrait"
      android:theme="@style/DashboardTheme"
      android:windowSoftInputMode="adjustPan" />
  </application>
</manifest>