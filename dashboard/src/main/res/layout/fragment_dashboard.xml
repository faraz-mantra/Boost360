<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools">

  <data>

  </data>

  <androidx.core.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fillViewport="true">

    <androidx.appcompat.widget.LinearLayoutCompat
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:orientation="vertical"
      android:paddingBottom="@dimen/size_16">

      <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <FrameLayout
          android:layout_width="match_parent"
          android:layout_height="@dimen/size_200">

          <com.framework.views.customViews.CustomImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/ic_bg_dash" />

          <com.framework.views.customViews.CustomImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/ic_fill_bg" />
        </FrameLayout>

        <com.framework.views.customViews.CustomCardView
          android:layout_width="match_parent"
          android:layout_height="@dimen/size_226"
          android:layout_marginStart="@dimen/size_6"
          android:layout_marginTop="@dimen/size_122"
          android:layout_marginEnd="@dimen/size_6"
          app:cardBackgroundColor="@color/white"
          app:cardCornerRadius="@dimen/size_16"
          app:cardUseCompatPadding="true">

          <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.appcompat.widget.LinearLayoutCompat
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">

              <LinearLayout
                android:id="@+id/view_visiting_card_product"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/size_3"
                android:orientation="horizontal"
                android:visibility="gone">

                <LinearLayout
                  android:id="@+id/btn_visiting_card_down"
                  android:layout_width="0dp"
                  android:layout_height="@dimen/size_42"
                  android:layout_marginEnd="1.5dp"
                  android:layout_weight="1"
                  android:background="@drawable/ic_bg_btn_round_start"
                  android:gravity="center"
                  android:orientation="horizontal">

                  <com.framework.views.customViews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_visiting_card"
                    android:textColor="@color/greyish_brown"
                    app:fontFamily="@font/sans_medium" />

                  <com.framework.views.customViews.CustomImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_6"
                    android:layout_marginTop="@dimen/size_1"
                    app:srcCompat="@drawable/ic_arrow_down_7_d" />
                </LinearLayout>

                <LinearLayout
                  android:id="@+id/btn_product_catalog_down"
                  android:layout_width="0dp"
                  android:layout_height="@dimen/size_42"
                  android:layout_marginStart="1.5dp"
                  android:layout_weight="1"
                  android:background="@drawable/ic_bg_btn_round_end"
                  android:gravity="center">

                  <com.framework.views.customViews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_products_catalog"
                    android:textColor="@color/greyish_brown"
                    app:fontFamily="@font/sans_medium" />

                  <com.framework.views.customViews.CustomImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/size_6"
                    android:layout_marginTop="@dimen/size_1"
                    app:srcCompat="@drawable/ic_arrow_down_7_d" />
                </LinearLayout>
              </LinearLayout>

              <LinearLayout
                android:id="@+id/view_all_business_contact"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/size_12"
                android:orientation="horizontal">

                <LinearLayout
                  android:id="@+id/btn_visiting_card_up"
                  android:layout_width="0dp"
                  android:layout_height="wrap_content"
                  android:layout_marginStart="@dimen/size_2"
                  android:layout_weight="1"
                  android:orientation="vertical">

                  <com.framework.views.customViews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableEnd="@drawable/ic_arrow_up_d"
                    android:drawablePadding="@dimen/size_8"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="@string/my_visiting_card"
                    android:textColor="@color/colorAccent"
                    app:textType="HEADING_6" />

                  <com.framework.views.customViews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_2"
                    android:ellipsize="end"
                    android:maxLines="2"
                    android:text="@string/one_tap_to_share_your_all_business_contact_points_with_anyone"
                    android:textColor="@color/warm_grey"
                    app:textType="CAPTION" />
                </LinearLayout>

                <com.framework.views.customViews.CustomImageView
                  android:layout_width="@dimen/size_28"
                  android:layout_height="@dimen/size_28"
                  android:background="?attr/selectableItemBackgroundBorderless"
                  android:clickable="true"
                  android:focusable="true"
                  android:padding="@dimen/size_4"
                  android:id="@+id/btn_digital_channel"
                  app:srcCompat="@drawable/ic_filter_d" />
              </LinearLayout>

            </androidx.appcompat.widget.LinearLayoutCompat>

            <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_weight="1"
              android:gravity="center_vertical">

              <com.framework.views.viewgroups.BaseRecyclerView
                android:id="@+id/rv_channel_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:itemCount="4"
                tools:listitem="@layout/item_channel_d" />
            </LinearLayout>

            <View
              android:layout_width="match_parent"
              android:layout_height="@dimen/size_1"
              android:layout_marginTop="@dimen/size_8"
              android:background="@color/grey_dim" />

            <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="@dimen/size_42"
              android:background="@color/light_grey_5"
              android:orientation="horizontal">

              <com.framework.views.customViews.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center"
                android:text="@string/share_on_whatsapp"
                android:textColor="@color/black_4f4f4f"
                app:fontFamily="@font/sans_medium" />

              <View
                android:layout_width="@dimen/size_1"
                android:layout_height="match_parent"
                android:background="@color/grey_dim" />

              <com.framework.views.customViews.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center"
                android:text="@string/share_on_more"
                android:textColor="@color/black_4f4f4f"
                app:fontFamily="@font/sans_medium" />
            </LinearLayout>
          </androidx.appcompat.widget.LinearLayoutCompat>

        </com.framework.views.customViews.CustomCardView>

        <androidx.appcompat.widget.LinearLayoutCompat
          android:id="@+id/view_digital_score"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="@dimen/size_174"
          android:background="@drawable/ic_bg_dark_white_top_round"
          android:orientation="vertical"
          android:paddingTop="@dimen/size_164"
          tools:background="@drawable/ic_bg_white_top_round">

          <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/view_low_digital_readiness"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/size_14"
            android:orientation="vertical"
            android:visibility="gone">

            <androidx.appcompat.widget.LinearLayoutCompat
              android:id="@+id/btn_show_digital_score"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginStart="@dimen/size_14"
              android:layout_marginTop="@dimen/size_20"
              android:layout_marginEnd="@dimen/size_12"
              android:clickable="true"
              android:focusable="true"
              android:orientation="horizontal">

              <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_2"
                android:orientation="horizontal">

                <com.framework.views.customViews.CustomTextView
                  android:id="@+id/txt_readiness_score"
                  android:layout_width="wrap_content"
                  android:layout_height="match_parent"
                  android:layout_marginTop="-13dp"
                  android:includeFontPadding="false"
                  android:lineSpacingExtra="0dp"
                  android:textColor="@color/accent_dark"
                  app:fontFamily="@font/light"
                  app:textType="HEADING_TEXT_2"
                  tools:text="26" />

                <com.framework.views.customViews.CustomTextView
                  android:layout_width="wrap_content"
                  android:layout_height="match_parent"
                  android:gravity="top"
                  android:includeFontPadding="false"
                  android:text="%"
                  android:textColor="@color/accent_dark"
                  app:fontFamily="@font/semi_bold"
                  app:textType="HEADING_6" />
              </androidx.appcompat.widget.LinearLayoutCompat>

              <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/size_6"
                android:layout_marginEnd="@dimen/size_6"
                android:layout_weight="1"
                android:orientation="vertical">

                <com.framework.views.customViews.CustomTextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="@string/digital_readiness_score"
                  android:textColor="@color/black_4f4f4f"
                  app:fontFamily="@font/sans_medium"
                  app:textType="HEADING_6" />

                <com.framework.views.customViews.CustomTextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="@dimen/size_2"
                  android:ellipsize="end"
                  android:maxLines="2"
                  android:text="@string/improve_your_score_to_90"
                  android:textColor="@color/warm_grey"
                  app:textType="OVERLINE" />
              </androidx.appcompat.widget.LinearLayoutCompat>

              <com.framework.views.customViews.CustomTextView
                android:layout_width="@dimen/size_82"
                android:layout_height="@dimen/size_26"
                android:layout_gravity="center_vertical"
                android:background="@drawable/ic_round_accent_d"
                android:drawableEnd="@drawable/ic_arrow_right_7_white_d"
                android:drawablePadding="-6dp"
                android:gravity="center"
                android:includeFontPadding="false"
                android:paddingEnd="@dimen/size_6"
                android:text="@string/improve"
                android:textAllCaps="true"
                android:textColor="@color/white"
                app:fontFamily="@font/semi_bold"
                app:textType="CAPTION"
                tools:ignore="RtlSymmetry" />
            </androidx.appcompat.widget.LinearLayoutCompat>

            <ProgressBar
              android:id="@+id/progress_score"
              style="@style/CustomProgressBar"
              android:layout_width="match_parent"
              android:layout_height="@dimen/size_6"
              android:layout_marginStart="@dimen/size_16"
              android:layout_marginTop="@dimen/size_16"
              android:layout_marginEnd="@dimen/size_12"
              android:max="100"
              android:progress="50" />

            <androidx.appcompat.widget.LinearLayoutCompat
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">

              <androidx.constraintlayout.motion.widget.MotionLayout
                android:id="@+id/motion_one"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_16"
                app:layoutDescription="@xml/fragment_dashboard_scene">

                <androidx.appcompat.widget.LinearLayoutCompat
                  android:id="@+id/view_missing"
                  android:layout_width="0dp"
                  android:layout_height="wrap_content"
                  android:gravity="top|center_horizontal"
                  android:orientation="vertical"
                  android:paddingStart="@dimen/size_4"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintHorizontal_chainStyle="spread"
                  app:layout_constraintLeft_toLeftOf="parent"
                  app:layout_constraintRight_toLeftOf="@+id/view_pager"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintWidth_default="percent"
                  app:layout_constraintWidth_percent="0.26"
                  tools:ignore="RtlSymmetry">

                  <com.framework.views.customViews.CustomCardView
                    android:layout_width="@dimen/size_80"
                    android:layout_height="@dimen/size_80"
                    android:layout_margin="@dimen/size_2"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/size_40">

                    <com.framework.views.customViews.CustomImageView
                      android:id="@+id/missing_details_gif"
                      android:layout_width="match_parent"
                      android:layout_height="match_parent"
                      android:padding="@dimen/size_18"
                      app:srcCompat="@drawable/ic_custom_page_d" />
                  </com.framework.views.customViews.CustomCardView>

                  <com.framework.views.customViews.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_16"
                    android:gravity="center"
                    android:text="@string/missing_ndetails"
                    android:textColor="@color/grey_dim_1"
                    app:fontFamily="@font/sans_medium"
                    app:textType="HEADING_6" />
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                  android:id="@+id/view_arrow"
                  android:layout_width="0dp"
                  android:layout_height="wrap_content"
                  android:orientation="vertical"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintHorizontal_chainStyle="spread"
                  app:layout_constraintRight_toLeftOf="@+id/view_pager"
                  app:layout_constraintWidth_default="percent"
                  app:layout_constraintWidth_percent="0.12">

                  <com.framework.views.customViews.CustomImageView
                    android:id="@+id/arrow_left_gif"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/size_14"
                    android:layout_gravity="end"
                    android:layout_marginBottom="@dimen/size_16"
                    app:srcCompat="@drawable/ic_arrow_right_14_d" />
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                  android:id="@+id/view_pager"
                  android:layout_width="0dp"
                  android:layout_height="match_parent"
                  android:orientation="vertical"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintLeft_toRightOf="@+id/view_missing"
                  app:layout_constraintRight_toRightOf="parent"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintWidth_default="percent"
                  app:layout_constraintWidth_percent="1">

                  <androidx.viewpager2.widget.ViewPager2
                    android:id="@+id/pager_business_setup_low"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/size_160"
                    android:layout_marginStart="@dimen/size_2"
                    android:clipChildren="false"
                    android:clipToPadding="false" />
                </androidx.appcompat.widget.LinearLayoutCompat>

              </androidx.constraintlayout.motion.widget.MotionLayout>

              <com.framework.views.dotsindicator.DotsIndicator
                android:id="@+id/dot_indicator"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/size_10"
                android:gravity="center"
                app:dotsColor="@color/light_grey_1"
                app:dotsCornerRadius="@dimen/_6dp"
                app:dotsSize="@dimen/size_5"
                app:dotsSpacing="@dimen/size_2"
                app:dotsWidthFactor="2.5"
                app:progressMode="false"
                app:selectedDotColor="@color/grey_dark_2" />
            </androidx.appcompat.widget.LinearLayoutCompat>

          </androidx.appcompat.widget.LinearLayoutCompat>

          <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/view_high_digital_readiness"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.LinearLayoutCompat
              android:id="@+id/view_business_bg_score"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:background="@drawable/ic_bg_dark_white_vertical"
              android:orientation="vertical">

              <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_18"
                android:orientation="vertical">

                <androidx.viewpager2.widget.ViewPager2
                  android:id="@+id/pager_business_setup_high"
                  android:layout_width="match_parent"
                  android:layout_height="@dimen/size_50"
                  android:clipChildren="false"
                  android:clipToPadding="false" />

                <com.framework.views.dotsindicator.DotsIndicator
                  android:id="@+id/dot_indicator_business_high"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="center_horizontal"
                  android:layout_marginTop="@dimen/size_14"
                  app:dotsColor="@color/light_grey_1"
                  app:dotsCornerRadius="@dimen/_6dp"
                  app:dotsSize="@dimen/size_5"
                  app:dotsSpacing="@dimen/size_2"
                  app:dotsWidthFactor="2.5"
                  app:progressMode="false"
                  app:selectedDotColor="@color/colorAccent" />
              </androidx.appcompat.widget.LinearLayoutCompat>

              <View
                android:id="@+id/view_up_business_shadow"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size_6"
                android:layout_marginTop="@dimen/size_6"
                android:background="@android:color/transparent"
                tools:background="@drawable/up_shadow_d" />
            </androidx.appcompat.widget.LinearLayoutCompat>

            <include
              android:id="@+id/high_manage_business"
              layout="@layout/item_manage_business_view" />

            <include
              android:id="@+id/high_recommended_task"
              layout="@layout/item_recommended_task_view" />

          </androidx.appcompat.widget.LinearLayoutCompat>

        </androidx.appcompat.widget.LinearLayoutCompat>

        <androidx.appcompat.widget.LinearLayoutCompat
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="@dimen/size_34"
          android:orientation="horizontal">

          <com.framework.views.customViews.CustomCardView
            android:layout_width="@dimen/size_66"
            android:layout_height="@dimen/size_66"
            android:layout_marginStart="@dimen/size_14"
            app:cardBackgroundColor="@color/snow_white"
            app:cardCornerRadius="@dimen/size_6">

            <com.framework.views.customViews.CustomCardView
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_margin="@dimen/size_4"
              app:cardBackgroundColor="@color/white_one"
              app:cardCornerRadius="@dimen/size_6"
              app:cardElevation="@dimen/size_0">

              <com.framework.views.customViews.CustomImageView
                android:id="@+id/img_business_logo"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                app:srcCompat="@drawable/ic_add_logo_d" />
            </com.framework.views.customViews.CustomCardView>

          </com.framework.views.customViews.CustomCardView>

          <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/size_6"
            android:layout_marginEnd="@dimen/size_6"
            android:layout_weight="1"
            android:orientation="vertical">

            <com.framework.views.customViews.CustomTextView
              android:id="@+id/txt_business_name"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:ellipsize="end"
              android:includeFontPadding="false"
              android:lineSpacingExtra="-6dp"
              android:maxLines="2"
              android:text="PLASTO INDIA AND POLYMERS"
              android:textColor="@color/white"
              app:fontFamily="@font/bold"
              app:textType="HEADING_5" />

            <com.framework.views.customViews.CustomTextView
              android:id="@+id/txt_domain_name"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginStart="@dimen/size_2"
              android:drawableStart="@drawable/ic_edit_d"
              android:drawablePadding="@dimen/size_4"
              android:ellipsize="end"
              android:includeFontPadding="false"
              android:maxLines="1"
              android:text="@string/www_sravansclinic_com"
              android:textColor="@color/greyish_brown"
              app:textType="BODY_1" />
          </androidx.appcompat.widget.LinearLayoutCompat>

          <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/size_4"
            android:layout_marginEnd="@dimen/size_12"
            android:background="?attr/selectableItemBackgroundBorderless"
            android:clickable="true"
            android:focusable="true">

            <com.framework.views.customViews.CustomImageView
              android:layout_width="@dimen/size_34"
              android:layout_height="@dimen/size_34"
              android:layout_marginStart="@dimen/size_8"
              android:layout_marginTop="@dimen/size_8"
              android:background="@drawable/ic_chat_ria_icon_d" />

            <com.framework.views.customViews.CustomTextView
              android:id="@+id/txt_notification"
              android:layout_width="@dimen/size_26"
              android:layout_height="@dimen/size_16"
              android:background="@drawable/ic_round_red_d"
              android:gravity="center"
              android:paddingStart="@dimen/size_2"
              android:textAlignment="gravity"
              android:textColor="@color/white"
              app:textType="OVERLINE"
              tools:ignore="RtlSymmetry"
              tools:text="9+" />
          </FrameLayout>
        </androidx.appcompat.widget.LinearLayoutCompat>
      </FrameLayout>

      <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_12"
        android:orientation="vertical">

        <LinearLayout
          android:id="@+id/view_low_task_manage_business"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:orientation="vertical">

          <include
            android:id="@+id/low_recommended_task"
            layout="@layout/item_recommended_task_view" />

          <include
            android:id="@+id/low_manage_business"
            layout="@layout/item_manage_business_view" />
        </LinearLayout>


        <androidx.appcompat.widget.LinearLayoutCompat
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:orientation="vertical">

          <com.framework.views.customViews.CustomTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/size_16"
            android:layout_marginEnd="@dimen/size_16"
            android:text="@string/ria_academy"
            android:textColor="@color/black_4f4f4f"
            app:fontFamily="@font/sans_medium"
            app:textType="HEADING_6" />

          <com.framework.views.customViews.CustomTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/size_16"
            android:layout_marginTop="@dimen/size_2"
            android:layout_marginEnd="@dimen/size_16"
            android:ellipsize="end"
            android:maxLines="2"
            android:text="@string/learn_boost_360"
            android:textColor="@color/warm_grey"
            app:textType="CAPTION" />

          <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/pager_ria_academy"
            android:layout_width="match_parent"
            android:layout_height="@dimen/size_130"
            android:layout_marginTop="@dimen/size_14"
            android:clipChildren="false"
            android:clipToPadding="false" />

          <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/view_high_summary_bottom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.LinearLayoutCompat
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:background="@drawable/ic_bg_dark_white_round"
              android:orientation="vertical">

              <FrameLayout
