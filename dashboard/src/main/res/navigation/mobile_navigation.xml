<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/navigation_dashboard">

  <fragment
    android:id="@+id/navigation_dashboard"
    android:name="com.dashboard.controller.ui.dashboard.DashboardFragment"
    android:label="@string/dashboard"
    tools:layout="@layout/fragment_dashboard" />

  <fragment
    android:id="@+id/navigation_website"
    android:name="com.dashboard.controller.ui.website.WebsiteV2Fragment"
    android:label="@string/my_website"
    tools:layout="@layout/fragment_website_v2" />

  <fragment
    android:id="@+id/navigation_enquiries"
    android:name="com.dashboard.controller.ui.enquiries.EnquiriesFragment"
    android:label="@string/my_enquiry"
    tools:layout="@layout/fragment_patients_customer" />
  <fragment
    android:id="@+id/more_settings"
    android:name="com.dashboard.controller.ui.more.MoreFragment"
    android:label="@string/menu"
    tools:layout="@layout/fragment_more" />
</navigation>