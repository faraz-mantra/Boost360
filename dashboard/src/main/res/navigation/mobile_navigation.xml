<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/mobile_navigation"
  app:startDestination="@+id/navigation_dashboard">

  <fragment
    android:id="@+id/navigation_dashboard"
    android:name="com.dashboard.controller.ui.dashboard.DashboardFragment"
    android:label="@string/dashboard"
    tools:layout="@layout/fragment_dashboard" />

  <fragment
    android:id="@+id/navigation_website"
    android:name="com.dashboard.controller.ui.website.WebsiteFragment"
    android:label="@string/my_website"
    tools:layout="@layout/fragment_website" />

  <fragment
    android:id="@+id/navigation_customer"
    android:name="com.dashboard.controller.ui.customer.PatientCustomerFragment"
    android:label="@string/my_enquiry"
    tools:layout="@layout/fragment_patients_customer" />

</navigation>