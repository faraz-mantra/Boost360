      <TextView
        android:id="@+id/tvManageContent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/manage_content"
        android:textColor="@color/cell_text_color"
        android:textSize="@dimen/cell_text_size" />

    </LinearLayout>

    <LinearLayout
      android:id="@+id/twelveth_Layout"
