        android:layout_marginEnd="@dimen/icon_right_margin"
        android:padding="5dp"
        android:scaleType="fitXY"
        android:src="@drawable/catalog" />

      <TextView
        android:id="@+id/Product_Gal_TextView"
        android:layout_width="wrap_content"
