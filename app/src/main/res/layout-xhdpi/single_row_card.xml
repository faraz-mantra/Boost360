                app:srcCompat="@drawable/ic_nav_enquiries" />

            <TextView
                android:id="@+id/tvInbox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/manage_inbox"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>


        <LinearLayout
