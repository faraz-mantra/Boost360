                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="35dp"
                    android:background="@drawable/write_to_ria_info_bg"
                    android:gravity="center"
                    android:text="i"
                    android:textColor="#ffffff"
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_get_in_touch"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
                android:id="@+id/iv_get_in_touch"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:padding="5dp"
                android:scaleType="fitXY"
                android:src="@drawable/icon_feedback"
                android:tint="#808080" />

            <TextView
                android:id="@+id/tv_get_in_touch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="@string/feedback"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/seventhRow_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/seventhRow_ImageView"
                android:layout_width="@dimen/nav_icon_size"
                android:layout_height="@dimen/nav_icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_nav_help_support" />

            <TextView
                android:id="@+id/seventhRow_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/help_and_support"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:visibility="visible"
            android:background="#cccccc" />

        <LinearLayout
            android:id="@+id/layout_about"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_about"
                android:layout_width="@dimen/nav_icon_size"
                android:layout_height="@dimen/nav_icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_nav_about" />

            <TextView
                android:id="@+id/tv_about"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/about"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/eigthRow_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
                android:id="@+id/eigthRow_ImageView"
                android:layout_width="@dimen/nav_icon_size"
                android:layout_height="@dimen/nav_icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:scaleType="fitXY"
                android:src="@drawable/ic_nav_share_site" />

            <TextView
                android:id="@+id/eighthRow_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/side_panel_row_eight"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>

    </LinearLayout>
</ScrollView>