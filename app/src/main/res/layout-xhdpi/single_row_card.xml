            android:orientation="horizontal">

            <ImageView
                android:id="@+id/keyboard_ImageView"
                android:layout_width="@dimen/nav_icon_size"
                android:layout_height="@dimen/nav_icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_nav_keyboard" />

            <TextView
                android:id="@+id/keyboard_TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
