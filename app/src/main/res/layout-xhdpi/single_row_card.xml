                    android:layout_marginRight="@dimen/icon_right_margin"
                    android:padding="5dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/lock_widget"
                    android:visibility="gone" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/fourthRow_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
