                android:layout_width="@dimen/nav_icon_size"
                android:layout_height="@dimen/nav_icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_nav_calls" />

            <TextView
                android:id="@+id/tvCustomerCalls"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/manage_customer_calls"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/thirteen_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
