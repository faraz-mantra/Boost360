                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/addon_marketplace"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>

        <View
