            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/marketplace_ImageView"
                android:layout_width="@dimen/nav_icon_size"
                android:layout_height="@dimen/nav_icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_addons_store" />

            <TextView
                android:id="@+id/marketplace_TextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Add-Ons Store"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

            <TextView
                fontPath="Roboto-Regular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="27dp"
                android:layout_gravity="center_vertical"
                android:layout_marginRight="15dp"
                android:background="@drawable/addons_store_new_bg"
