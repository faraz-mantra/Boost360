                android:layout_width="@dimen/icon_size"
                android:layout_height="@dimen/icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:padding="5dp"
                android:scaleType="fitXY"
                android:src="@drawable/sidepanel_gallery" />

            <TextView
                android:id="@+id/fourthRow_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/side_panel_row_four"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/lock_widget"
                    android:layout_width="@dimen/icon_size"
                    android:layout_height="@dimen/icon_size"
                    android:layout_marginLeft="@dimen/icon_left_margin"
                    android:layout_marginRight="@dimen/icon_right_margin"
                    android:padding="5dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/lock_widget"
                    android:visibility="gone" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/csp_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
                android:id="@+id/csp_ImageView"
                android:layout_width="@dimen/icon_size"
                android:layout_height="@dimen/icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:padding="5dp"
                android:scaleType="fitXY"
                android:src="@drawable/custom_page_icon" />

            <TextView
                android:id="@+id/csp_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/side_panel_row_csp"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/lock_widget_csp"
                    android:layout_width="@dimen/icon_size"
                    android:layout_height="@dimen/icon_size"
                    android:layout_marginLeft="@dimen/icon_left_margin"
                    android:layout_marginRight="@dimen/icon_right_margin"
                    android:padding="5dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/lock_widget"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/storeRow_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
                android:id="@+id/storeRow_ImageView"
                android:layout_width="@dimen/icon_size"
                android:layout_height="@dimen/icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:padding="5dp"
                android:scaleType="fitXY"
                android:src="@drawable/sidepanel_store" />

            <TextView
                android:id="@+id/storeRow_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/store"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:gravity="right">

                <Button
                    fontPath="Roboto-Regular.ttf"
                    android:layout_width="80dp"
                    android:layout_height="27dp"
                    android:layout_gravity="right"
                    android:background="@drawable/corner_white_backgrnd_yellow"
                    android:clickable="false"
                    android:padding="3dp"
                    android:text="@string/single_row_card_upgrade_text"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/thirdRow_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:visibility="gone"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/thirdRow_ImageView"
                android:layout_width="@dimen/icon_size"
                android:layout_height="@dimen/icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
                android:layout_marginRight="@dimen/icon_right_margin"
                android:padding="12dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_add_ons" />

            <TextView
                android:id="@+id/thirdRow_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_ons"
                android:textColor="@color/cell_text_color"
                android:textSize="@dimen/cell_text_size" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/secondRow_Layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sidepanel_cell_height"
            android:background="@color/cell_background_color"
            android:gravity="center_vertical"
            android:visibility="gone"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/secondRow_ImageView"
                android:layout_width="@dimen/icon_size"
                android:layout_height="@dimen/icon_size"
                android:layout_marginLeft="@dimen/icon_left_margin"
