<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:background="@color/white">

  <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="horizontal">

    <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginEnd="@dimen/margin_10"
      android:background="@color/white"
      android:orientation="vertical">

      <ImageView
        android:layout_width="@dimen/icon_size_55"
        android:layout_height="@dimen/icon_size_55"
        android:padding="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_business_enquiry"
        android:visibility="gone" />

      <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@null" />

      <ImageView
        android:layout_width="@dimen/icon_size_55"
        android:layout_height="@dimen/icon_size_55"
        android:padding="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_business_calls"
        android:visibility="gone" />

      <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@null" />

      <ImageView
        android:id="@+id/transactions_type_1_image"
        android:layout_width="@dimen/icon_size_55"
        android:layout_height="@dimen/icon_size_55"
        android:padding="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_all_orders" />


      <ImageView
        android:id="@+id/transactions_type_2_image"
        android:layout_width="@dimen/icon_size_55"
        android:layout_height="@dimen/icon_size_55"
        android:padding="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_product_gallery" />

      <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@null" />

      <ImageView
        android:layout_width="@dimen/icon_size_55"
        android:layout_height="@dimen/icon_size_55"
        android:padding="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_order_summary_new" />

      <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@null" />

      <ImageView
        android:id="@+id/book_a_table_icon"
        android:layout_width="@dimen/icon_size_55"
        android:layout_height="@dimen/icon_size_55"
        android:padding="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_order_summary_new" />

      <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@null" />

    </LinearLayout>

    <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@color/white"
      android:orientation="vertical">

      <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d3d3d3"
        android:visibility="gone" />

      <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
          android:id="@+id/transactions_type_1"
          android:layout_width="0dp"
          android:layout_height="@dimen/cell_height"
          android:layout_weight="1"
          android:background="?android:selectableItemBackground"
          android:gravity="center_vertical"
          android:text="@string/seller_analytics"
          android:textColor="@color/cell_text_color"
          android:textSize="@dimen/cell_text_size" />

        <ImageView
          android:id="@+id/lock_widget"
          android:layout_width="@dimen/icon_size"
          android:layout_height="@dimen/icon_size"
          android:layout_gravity="center_vertical"
          android:layout_marginStart="@dimen/icon_left_margin"
          android:layout_marginEnd="@dimen/icon_right_margin"
          android:padding="6dp"
          android:scaleType="fitXY"
          android:src="@drawable/lock_widget"
          android:visibility="gone" />

      </LinearLayout>

      <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d3d3d3" />

      <TextView
        android:id="@+id/transactions_type_2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/cell_height"
        android:background="?android:selectableItemBackground"
        android:gravity="center_vertical"
        android:text="@string/product_gallery"
        android:textColor="@color/cell_text_color"
        android:textSize="14sp" />

      <View
        android:id="@+id/line_view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d3d3d3"
        android:visibility="visible" />

      <TextView
        android:id="@+id/tvOrderSummary"
        android:layout_width="match_parent"
        android:layout_height="@dimen/cell_height"
        android:background="?android:selectableItemBackground"
        android:gravity="center_vertical"
        android:text="@string/order_analytics_"
        android:textColor="@color/cell_text_color"
        android:textSize="@dimen/cell_text_size" />

      <View
        android:id="@+id/line_view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d3d3d3"
        android:visibility="visible" />

      <LinearLayout
        android:id="@+id/book_table"
        android:layout_width="match_parent"
        android:layout_height="@dimen/cell_height"
        android:background="?android:selectableItemBackground"
        android:gravity="center_vertical">

        <TextView
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:gravity="center_vertical"
          android:text="@string/book_a_table"
          android:textColor="@color/cell_text_color"
          android:textSize="@dimen/cell_text_size" />

        <ImageView
          android:id="@+id/feature_lock"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:paddingEnd="30dp"
          android:visibility="gone"
          app:srcCompat="@drawable/ic_expiry_lock"
          tools:visibility="visible" />


      </LinearLayout>

      <View
        android:id="@+id/line_view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d3d3d3"
        android:visibility="visible" />

    </LinearLayout>

  </LinearLayout>

</LinearLayout>