        android:layout_width="@dimen/icon_size"
        android:layout_height="@dimen/icon_size"
        android:layout_marginStart="@dimen/icon_left_margin"
        android:layout_marginEnd="@dimen/icon_right_margin"
        android:padding="5dp"
        android:scaleType="fitXY"
        android:src="@drawable/sidepanel_share" />

      <TextView
        android:id="@+id/eighthRow_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/side_panel_row_eight"
        android:textColor="@color/cell_text_color"
        android:textSize="@dimen/cell_text_size" />

    </LinearLayout>

    <LinearLayout
      android:id="@+id/ninethRow_Layout"
      android:layout_width="match_parent"
      android:layout_height="@dimen/sidepanel_cell_height"
      android:background="@color/cell_background_color"
      android:gravity="center_vertical"
      android:orientation="horizontal"
      android:visibility="gone">

      <ImageView
        android:id="@+id/ninethRow_ImageView"
        android:layout_width="@dimen/icon_size"
        android:layout_height="@dimen/icon_size"
        android:layout_marginStart="@dimen/icon_left_margin"
        android:layout_marginEnd="@dimen/icon_right_margin"
        android:padding="5dp"
        android:scaleType="fitXY"
        android:src="@drawable/icon_info" />

      <TextView
        android:id="@+id/ninethRow_TextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/boost_bubble"
        android:textColor="@color/cell_text_color"
        android:textSize="@dimen/cell_text_size" />

      <Switch
        android:id="@+id/ninethRow_Switch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|center"
        android:checked="false"
        android:padding="3dp"
        android:textColorHighlight="@color/product_price" />

    </LinearLayout>

  </LinearLayout>
</ScrollView>