<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:right="17dp">
    <rotate
      android:fromDegrees="-60.0"
      android:toDegrees="60.0">
      <shape android:shape="rectangle">
        <solid android:color="#ffff" />
        <corners
          android:bottomLeftRadius="0dp"
          android:bottomRightRadius="0dp"
          android:radius="1dp" />
        <size
          android:width="2dp"
          android:height="20dp" />
      </shape>
    </rotate>
  </item>
  <item android:left="17dp">
    <rotate
      android:fromDegrees="60.0"
      android:toDegrees="-60.0">
      <shape android:shape="rectangle">
        <solid android:color="#ffff" />
        <corners
          android:bottomLeftRadius="0dp"
          android:bottomRightRadius="0dp"
          android:radius="1dp" />
        <size
          android:width="2dp"
          android:height="20dp" />
      </shape>
    </rotate>
  </item>
</layer-list>