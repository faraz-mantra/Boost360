<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools">

  <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.framework.views.customViews.CustomImageView
      android:id="@+id/iv_mobile_overlay"
      android:layout_width="@dimen/size_300"
      android:layout_height="@dimen/size_200"
      android:layout_gravity="center"
      android:layout_marginTop="@dimen/size_4"
      android:src="@drawable/bacground_desktop_preview" />

    <com.framework.views.customViews.CustomImageView
      android:id="@+id/img_desktop"
      android:layout_width="@dimen/size_230"
      android:layout_height="@dimen/size_140"
      android:layout_gravity="center"
      android:scaleType="fitXY"
      tools:srcCompat="@drawable/mobile_preview_website" />
  </FrameLayout>
</layout>