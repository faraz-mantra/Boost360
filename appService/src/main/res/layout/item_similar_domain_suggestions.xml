<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <com.framework.views.customViews.CustomTextView
            android:id="@+id/tv_domain_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:includeFontPadding="false"
            android:text="samplebusinesswebsite.com"
            android:textColor="@color/black_4a4a4a"
            android:textSize="@dimen/text_14" />

        <com.framework.views.customViews.CustomRadioButton
            android:id="@+id/radio_similar_domain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:button="@drawable/check_selector_domain_booking" />

    </RelativeLayout>
</layout>