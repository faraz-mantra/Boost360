<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools">

  <data />

  <androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <com.framework.views.customViews.CustomImageView
      android:id="@+id/customImageView2"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:src="@drawable/ic_no_product_stencil"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHeight_percent="0.4"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.151"
      app:layout_constraintWidth_percent="0.65" />

    <com.framework.views.customViews.CustomTextView
      android:id="@+id/customTextView"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/open_sans"
      android:text="@string/add_services_to_your_catalog"
      android:textColor="@color/black_4a4a4a"
      app:fontFamily="@font/regular"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/customImageView2"
      app:layout_constraintVertical_bias="0.055"
      app:textType="HEADING_7" />

    <com.framework.views.customViews.CustomTextView
      android:id="@+id/ctv_add_service_subheading"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginHorizontal="@dimen/size_30"
      android:gravity="center"
      android:stateListAnimator="@null"
      android:textAlignment="center"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/customTextView"
      app:layout_constraintVertical_bias="0.039"
      tools:text="@string/you_don_t_have_any_service_added_to_your_digital_catalog_as_of_yet_watch_video" />

    <com.framework.views.customViews.CustomButton
      android:id="@+id/cb_add_service"
      style="@style/ButtonStyle.Rounded.Solid"
      android:layout_width="match_parent"
      android:layout_height="@dimen/size_36"
      android:layout_marginHorizontal="@dimen/size_60"
      android:gravity="center"
      android:stateListAnimator="@null"
      android:text="@string/add_a_service"
      android:textAllCaps="true"
      android:textSize="14sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/ctv_add_service_subheading"
      app:layout_constraintVertical_bias="0.373" />

    <com.framework.views.customViews.CustomButton
      android:id="@+id/cb_setup_appointment"
      android:layout_width="wrap_content"
      android:layout_height="@dimen/size_36"
      android:background="@null"
      android:gravity="center"
      android:paddingHorizontal="@dimen/size_60"
      android:stateListAnimator="@null"
      android:text="@string/setup_appointment"
      android:textAllCaps="true"
      android:textColor="@color/yellow_ffb900"
      android:textSize="14sp"
      android:visibility="invisible"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.496"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/cb_add_service"
      app:layout_constraintVertical_bias="0.132" />
  </androidx.constraintlayout.widget.ConstraintLayout>
</layout>