<layout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto">

  <data>

  </data>

  <androidx.appcompat.widget.LinearLayoutCompat
    android:id="@+id/ll_timing_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/size_8"
    android:orientation="horizontal">

    <com.framework.views.customViews.CustomImageView
      android:layout_width="@dimen/size_5"
      android:layout_height="@dimen/size_5"
      android:layout_gravity="center"
      android:src="@drawable/ic_dot_green"
      android:tint="@color/blue_4A90E2" />

    <com.framework.views.customViews.CustomTextView
      android:id="@+id/ctv_timing_services"
      style="@style/StaffProfileTextContent"
      android:layout_width="0dp"
      android:layout_weight="1"
      android:maxLines="1"
      android:paddingStart="@dimen/size_8"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/customImageView" />
  </androidx.appcompat.widget.LinearLayoutCompat>
</layout>